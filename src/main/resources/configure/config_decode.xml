    <model>
        <pos>general-en-pos.xz</pos>
        <dep>general-en-dep.xz</dep>
    </model>

    <reader type="raw">
    </reader>

    <reader type="line">
    </reader>

    <reader type="tsv">
        <column index="1" field="id"/>
